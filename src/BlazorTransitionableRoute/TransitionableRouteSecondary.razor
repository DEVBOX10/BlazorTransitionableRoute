@inherits TransitionableRoutePrimary

@if (CanRender)
{
    <CascadingValue Value="RouteData">
        <CascadingValue Value="TransitioningIn">
            @ChildContent
        </CascadingValue>
    </CascadingValue>
}


@code {
    internal bool CanRender => RouteData != null;

    protected override void OnInitialized()
    {
        MakeSecondary();
        base.OnInitialized();
    }
}